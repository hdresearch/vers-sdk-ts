{
  "dxt_version": "0.1",
  "name": "vers-mcp",
  "version": "0.1.0-alpha.3",
  "description": "The official MCP Server for the Vers API",
  "author": {
    "name": "Vers",
    "email": "support@hdr.is"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdresearch/vers-sdk-ts.git"
  },
  "homepage": "https://github.com/hdresearch/vers-sdk-ts/tree/main/packages/mcp-server#readme",
  "documentation": "https://hdr.is",
  "server": {
    "type": "node",
    "entry_point": "${__dirname}/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "VERS_API_KEY": "${user_config.VERS_API_KEY}"
      }
    }
  },
  "user_config": {
    "VERS_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": false,
      "type": "string"
    }
  },
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
